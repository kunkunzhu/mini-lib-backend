{
  "name": "my-lib-backend",
  "version": "1.0.0",
  "description": "backend for my-lib project",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:ui": "rm -rf build && cd ../my-lib-frontend/ && npm run build && cp -r build ../my-lib-backend",
    "deploy": "git push heroku main",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "lint": "eslint ."
  },
  "author": "kun zhu",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.1.2"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.27.1",
    "nodemon": "^2.0.15"
  }
}
